<div class="searchResultDiv <%= last ? '' : 'searchResultDivNotLast' %>">
<% 	# Obtain a picture from the gallery
	@pictures = Picture.find_all_by_gallery_id(result.id)
	@picture = @pictures[0]
%>
	
	<%= count.to_s %>. <%= link_to image_tag(getThumbnail(@picture.imageURL), :height => 100), result %>
	<span class="searchResultItem"><%= link_to result.title, result%></span> <br />
</div>